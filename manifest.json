{
  "name": "Main Page Fix",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "resources/favicon-16x16.png",
    "32": "resources/favicon-32x32.png",
    "64": "resources/favicon.ico"
  },
  "description": "A Chrome extension to improve the homepage for personal use.",
  "chrome_url_overrides": {
    "newtab": "BetterNewTab.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}